<%= form_with model: [:admin, @place] do |f| %>
  <%= render 'shared/error_messages', target: @place %>
  <%= render partial: 'nodes/outer_select', locals: { f: f, outer: 'place_taxon', node: @place.place_taxon } %>
  <%= f.text_field :name %>
  <%= f.color_field :color %>
  <%= f.submit %>
<% end %>
