<div class="ui modal" id="modal">
  <i class="fas fa-times close"></i>
  <div class="ui header blue">
    <%= t('.edit') %>
  </div>

  <div class="content">
    <%= form_with model: @event, scope: 'event', url: admin_event_path(@event) do |f| %>
      <%= render partial: 'form', locals: { f: f } %>
    <% end %>
  </div>
</div>
