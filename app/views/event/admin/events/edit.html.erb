<%= render 'nav_event' %>

<div class="ui basic segment breadcrumb">
  <%= link_to 'Back', admin_events_path, class: 'section' %>
  <i class="fas fa-angle-right divider"></i>
  <%= link_to 'Detail', admin_event_path(@event), class: 'section' %>
  <i class="fas fa-angle-right divider"></i>
  <div class="active section">Edit</div>
</div>

<div class="ui segment">
  <%= form_with model: @event, scope: 'event', url: admin_event_path(@event), local: true do |f| %>
    <%= render partial: 'form', locals: { f: f } %>
  <% end %>
</div>
