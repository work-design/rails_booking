<table class="ui celled table">
  <thead>
    <tr>
      <th class="one wide"></th>
      <% Plan.xx.each do |index, name| %>
        <th class="two wide"><%= name %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @time_list.time_items.each do |time_item| %>
      <tr>
        <td>
          <time data-format="HH:mm"><%= time_item.start_at.to_s(:rfc822) %></time>
          <span>~</span>
          <time data-format="HH:mm"><%= time_item.finish_at.to_s(:rfc822) %></time>
        </td>
        <% @plan_items.each do |index, items| %>
        <td>
          <div class="ui labels">
            <% items.each do |plan_item| %>
            <span class="ui label"><%= link_to plan_item.plan_on, admin_plan_item_plan_attenders_path(plan_item), text: true %></span>
            <% end %>
          </div>
        </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
