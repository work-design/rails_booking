<% @settings.except(:defaultDate).each do |key, value| %>
<% if value.is_a?(Hash) %>
calendar.setOption('<%= key %>', <%= raw value.to_json %>)
<% elsif value.is_a?(String) %>
calendar.setOption('<%= key %>', '<%= value %>')
<% else %>
calendar.setOption('<%= key %>', <%= value %>)
<% end %>
<% end %>
calendar.gotoDate('<%= @settings[:defaultDate] %>')
